  "scripts": {
    "android": "react-native run-android --mode=qaDebug",
    "start": "react-native start",
    "all": "npm run android && npm run ios",
    "android:build.dev": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "android:build.production": "cd android && ./gradlew clean && ./gradlew assembleProductionRelease",
    "android:build.qa": "cd android && ./gradlew clean && ./gradlew assembleQaRelease",
    "android:build.stage": "cd android && ./gradlew clean && ./gradlew assembleStageRelease",
    "android:bundle.all": "rm -rf node_modules/react-native-vision-camera/android/.cxx && rm -rf node_modules/react-native-vision-camera/android/build && rm -rf android/.gradle && rm -rf android/.idea && rm -rf android/app/build && npm install && npm run lint-fix && npm run prepare && npm run test && npm run android:clean && npm run clean && npm run android:bundle.stage && npm run android:bundle.qa && cd android && mv app/google-services.json app/----google-services.json && mv app/---google-services.json app/google-services.json && npm run android:bundle.production && mv app/google-services.json app/---google-services.json && mv app/----google-services.json app/google-services.json && cd ..",
    "android:bundle.production": "cd android && ./gradlew assembleProductionRelease --stacktrace && ./gradlew bundleProductionRelease --stacktrace && ./gradlew renameAabProduction",
    "android:bundle.qa": "cd android && ./gradlew assembleQaRelease --stacktrace",
    "android:bundle.stage": "cd android && ./gradlew assembleStageRelease --stacktrace",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:prd": "rm -rf node_modules/react-native-vision-camera/android/.cxx && rm -rf node_modules/react-native-vision-camera/android/build && rm -rf android/.gradle && rm -rf android/.idea && rm -rf android/app/build && npx react-native run-android --mode=productionDebug",
    "android:prd.full": "rm -rf node_modules && npm install && rm -rf android/app/build && npx react-native run-android --mode=productionRelease",
    "android:prod": "rm -rf node_modules && npm install && rm -rf android/app/build && npx react-native run-android --mode=productionDebug",
    "android:fix": "rm -rf node_modules && rm -rf android/.gradle && rm -rf android/.idea && npm install && cd android && ./gradlew build",
    "android:stage": "rm -rf node_modules && npm install && rm -rf android/app/build && npx react-native run-android --mode=stageDebug",
    "check-types": "tsc",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "force-clean": "npm run android:clean && npm run clean && rm -rf ios/build && rm -rf ios/Pods && pod cache clean --all && rm -rf node_modules/ && android/gradlew --stop && npm cache clean -f && npm install && cd ios/ && pod install",
    "postinstall": "patch-package && npm run lint-fix && npm run prepare && cd ios && pod install",
    "ios": "npx react-native run-ios --scheme dev",
    "ios:bundle.qa": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios && xcodebuild -workspace ios/Galicia.xcworkspace -scheme qa -archivePath ios/build/Galicia.xcarchive archive && xcodebuild -exportArchive -archivePath ios/build/Galicia.xcarchive -exportPath ios/build/Galicia-Office.ipa -exportOptionsPlist ios/Galicia/ExportOptions.plist",
    "ios:clean": "cd ios/ && rm -rf build && rm -rf Pods",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx",
    "lint-diff": "git diff --staged --name-only --relative --diff-filter=ACM | grep -E \"\\.(ts|tsx|js|jsx)$\" | xargs eslint",
    "lint-fix": "eslint --ext .js,.ts,.jsx,.tsx --fix",
    "prepare": "rm -rf ./lib && bob build",
    "prepare-install": "rm -rf lib && npm run ios:clean && npm run android:clean && rm -rf node_modules && npm install",
    "react-devtools": "react-devtools",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --env=jsdom --coverage --passWithNoTests --watchAll=false --silent --coverageThreshold \"{}\" --coverageReporters=\"json-summary\" && npm run test:generate-coverage-file",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
